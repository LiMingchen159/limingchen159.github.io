@article{thalfeldtFacadeDesignPrinciples2013,
  title = {Facade Design Principles for Nearly Zero Energy Buildings in a Cold Climate},
  author = {Thalfeldt, Martin and Pikas, Ergo and Kurnitski, Jarek and Voll, Hendrik},

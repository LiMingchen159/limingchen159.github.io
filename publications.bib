@article{quinnBuildingAutomationSystem2020,
  title = {Building Automation System - {{BIM}} Integration Using a Linked Data Structure},
  author = {Quinn, Caroline and Shabestari, Ali Zargar and Misic, Tony and Gilani, Sara and Litoiu, Marin and McArthur, J.J.},
  year = {2020},

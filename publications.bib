}

@phdthesis{TangHanLengAQuNongFangWaiChuangDeDuoMuBiaoYouHuaSheJiYanJiu2020,
  type = {{硕士}},
  title = {{寒冷A区农房外窗的多目标优化设计研究}},
  author = {汤, 兵},
  year = {2020},
  doi = {10.27273/d.cnki.gsajc.2020.000296},
  abstract = {当前城乡居民对于建筑居住环境的要求进一步提升,节能减排意识逐渐深入到建筑设计工作中,与建筑各项物理环境指标相关的一系列建筑性能化设计方法与技术得到了进一步发展。基于数字化技术和优化算法在建筑设计领域逐步得到广泛应用的趋势,本文以寒冷A区农房建筑外窗为研究对象,以建筑节能和自然采光作为性能驱动进行了相应的多目标优化为前提的设计研究。通过调研对比,研究首先确定了宁夏回族自治区吴忠市红寺堡区生态移民村农房为典型案例,对比最新的相关建筑节能与采光设计标准,通过项目验收数据与模拟分析明确了其存在典型的建筑性能多目标权衡的问题。在此基础上研究在参数化建模、建筑性能联合模拟、多目标优化算法等技术与理论的支撑下,设计了多目标优化实验(标准户型实验、附带阳光间户型实验)。通过对比选取了总建筑能耗、UDI\_(100-2000)、sDA三个建筑性能指标作为实验的优化目标,确定了不同性能参数的外窗类型与不同方向的窗墙比作为外窗性能与尺度设计变量。借助Grasshopper作为实验的编程操作和运行环境,利用Ladybug和Honeybee工具和Octopus工具生成了典型模型并完成了能够实现综合仿真模拟与优化的实验平台。在试运行以及检验工作完成后开展了多目标优化实验。通过对实验结果进行收敛性判断后,借助三维可视化分析得出了总建筑能耗、UDI\_(100-2000)、sDA三个指标彼此制约的结论。通过对Pareto前沿解集进行不同方式的筛选后得出了适用于不同户型的最佳外窗设计方案。基于数据整理与分析提出了适用于寒冷A区的农房外窗设计策略。本文的研究方法为国内农房的研究性设计提供了新的视角,弥补了农房建筑领域在多目标驱动设计与参数化设计方面存在的不足,为寒冷A区的农房外窗设计工作提供了支撑。},
  langid = {chinese},
  school = {山东建筑大学},
  keywords = {,A,exterior windows,farmhouses,multi-objective optimization,Pareto cold zone A,pareto front},
  annotation = {10 citations(CNKI)[3-20-2023]}
}


  title = {{{MFRED}}, 10 Second Interval Real and Reactive Power for Groups of 390 {{US}} Apartments of Varying Size and Vintage},
  author = {Meinrenken, Christoph J. and Rauschkolb, Noah and Abrol, Sanjmeet and Chakrabarty, Tuhin and Decalf, Victor C. and Hidey, Christopher and McKeown, Kathleen and Mehmani, Ali and Modi, Vijay and Culligan, Patricia J.},
  year = {2020},

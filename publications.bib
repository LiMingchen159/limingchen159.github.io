@article{LiuGuiXiBeiDiQuZhuangZuChuanTongMinJuDongJiShiNeiReHuanJingShiCeYanJiu2021,
  title = {{桂西北地区壮族传统民居冬季室内热环境实测研究}},
  author = {刘, 虹伶 and 杨, 真静 and 唐, 鸣放},
  year = {2021},
  journal = {建筑科学},

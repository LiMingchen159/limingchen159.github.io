@article{blumFieldDemonstrationImplementation2022,
  title = {Field Demonstration and Implementation Analysis of Model Predictive Control in an Office {{HVAC}} System},
  author = {Blum, David and Wang, Zhe and Weyandt, Chris and Kim, Donghun and Wetter, Michael and Hong, Tianzhen and Piette, Mary Ann},
  year = {2022},
  month = jul,
  journal = {Applied Energy},
  volume = {318},
  pages = {119104},
  issn = {0306-2619},
  doi = {10.1016/j.apenergy.2022.119104},
  urldate = {2023-02-04},
  abstract = {Model Predictive Control (MPC) is a promising technique to address growing needs for heating, ventilation, and air-conditioning (HVAC) systems to operate more efficiently and with greater flexibility. However, due to a number of factors, including the required implementation expertise, lack of high quality data, and a risk-adverse industry, MPC has yet to gain widespread adoption. While many previous studies have shown the advantages of MPC, few analyzed the implementation effort and associated practical challenges. In addition, previous work has developed an open-source, Modelica-based tool-chain that automatically generates optimal control, parameter estimation, and state estimation problems aimed at facilitating MPC implementation. Therefore, this study demonstrates usage of this tool-chain to implement MPC in a real office building, discusses practical challenges of implementing MPC, and estimates the implementation effort associated with various tasks in order to inform the development of future workflows and serve as an initial benchmark for their impact on reducing implementation effort. This study finds that the implemented MPC saves approximately 40\% of HVAC energy over the existing control during a two-month trial period and that tasks related to data collection and controller deployment activities can each require as much effort as model generation.},
  langid = {english},
  keywords = {HVAC,Model predictive control,Modelica,MPCPy,Optimization},
  annotation = {59 citations (Crossref/DOI) [2024-06-27]},
  file = {C\:\\Users\\liwei\\OneDrive\\Zotero\\MPC-RP\\Blum et al. - 2022 - Field demonstration and implementation analysis of model predictive control in an office HVAC system.pdf;C\:\\Users\\liwei\\Zotero\\storage\\QPB4BKAP\\S0306261922004895.html}
}


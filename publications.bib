  lccn = {1},
  keywords = {/unread,Computer architecture,Functional ANalysis Of VAriance (fANOVA),Handwriting recognition,Logic gates,long short-term memory (LSTM),Microprocessors,random search,recurrent neural networks,Recurrent neural networks,sequence learning,Speech recognition,Training},

@mastersthesis{jieApplicationResearchGreen2012,
  title = {{Application Research on Green Building Techniques Based on Grasshopper}},
  author = {Jie, Shen},
  year = {2012},
  urldate = {2022-03-16},
  abstract = {绿色建筑正在国内外建筑行业快速发展，在未来几年内将逐步成为全球建筑行业的主流，其设计原则也将成为几乎所有建筑设计必须遵循的基本原则。但在当前绿色建筑设计过程中，建筑设计和技术分析两者分工越来越独立，所使用的设计分析方法和软件工具分门别类，专业性很强而通用性不佳，整体协同方面有所欠缺。基于现状，本研究在参数化建筑设计过程中，引入技术分析和优化求解手段，为跨专业和跨平台的绿色建筑设计提供一种实用的全局优化和方案生成策略。与《民用建筑绿色设计规范》（JGJ/T229-2010）中对绿色建筑设计过程和要求的描述相同，本文提出的概念和方法重点针对各个设计环节的初期阶段，以量化分析方法对被动式绿色技术措施在方案中的应用进行优化设计。本文在建筑组团、单体建筑和局部构件三个不同尺度的设计层次，实现了建筑组团室外风环境优化、单体建筑节能优化和外遮阳构件优化三个具体应用。这些应用分别通过如下三步建立优化设计方案生成系统：（1）在Grasshopper设计平台中建立参数化建筑生成子系统；（2）使用公式、算法或导出到第三方软件进行逐个样本评价；（3）使用Galapagos遗传算法模块控制自变量与因变量形成自动优化过程。经过大量模拟计算和数据分析工作，这三个具体应用从设计模式的角度，对参数化设计和绿色建筑设计两者的结合进行详细论述，说明了其具有的``合理、有效、通用、普适、连续''五个特征。参数化建筑设计是一种高效的计算机辅助设计方法和工具，它不仅为绿色建筑设计提供了更多的设计可能性，极大得提高了设计效率，缩短了设计周期，而且它以其具有的严谨的逻辑性、量化的关联性等理性设计层面的内容，对绿色建筑设计来说具备得天独厚的优势和条件，必会给绿色建筑设计的发展带来新的强大动力。},
  collaborator = {Lihua, Zhao},
  langid = {chinese},
  school = {South China University of Technology},
  keywords = {,Design Optimize,Genetic Algorithm,Grasshopper,Green Building Design,Parametric Design}
}


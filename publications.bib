  author = {Chen, Zhelun and O'Neill, Zheng and Wen, Jin and Pradhan, Ojas and Yang, Tao and Lu, Xing and Lin, Guanjing and Miyata, Shohei and Lee, Seungjae and Shen, Chou and Chiosa, Roberto and Piscitelli, Marco Savino and Capozzoli, Alfonso and Hengel, Franz and K{\"u}hrer, Alexander and Pritoni, Marco and Liu, Wei and Clau{\ss}, John and Chen, Yimin and Herr, Terry},
  year = {2023},

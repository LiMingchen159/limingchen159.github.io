@article{liResearchBrickSchema2022,
  title = {Research on {{Brick Schema Representation}} for {{Building Operation}} with {{Variable Refrigerant Flow Systems}}},
  author = {Li, Jingming and Li, Nianping and Yan, Rui and Farruh, Kushnazarov and Li, Anbang and Li, Kehua},
  year = {2022},
  month = sep,
  journal = {Journal of Building Engineering},

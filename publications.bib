@phdthesis{CaoJiYuJianZhuJieNengDeXiaReDongLengDiQuBanGongJianZhuFuHeBiaoPiSheJiYanJiu2021,
  type = {{硕士}},
  title = {{基于建筑节能的夏热冬冷地区办公建筑复合表皮设计研究}},
  author = {曹, 航},
  year = {2021},
  doi = {10.27466/d.cnki.gzzdu.2021.004127},
  abstract = {作为公共建筑中数量多、建设面积大、使用频繁的一种建筑类型,办公建筑与社会上的每个人都有密不可分的关系,随着75\%节能标准的普及,办公建筑节能这一研究方向引来越来越多学者的关注。论文以夏热冬冷气候区的办公建筑为例,研究了复合表皮对于办公建筑单位面积能耗EUI的具体影响,通过参数化方式结合多目标优化对一拟建办公建筑能耗进行了模拟,总结了复合表皮的作用效果以及变化模式,为办公建筑节能与复合表皮设计提供了一定的参考价值。论文从夏热冬冷地区的气候特点出发,选取了夏热冬冷地区中人口密度较大的九个城市,统计其气象数据并总结出夏热冬冷地区的气候特点,结合办公建筑平面以及立面的设计形式与实地调研结果,提出建筑表皮设计对于夏热冬冷地区的办公建筑节能具有积极意义。随后论文对建筑表皮进行了深入研究,梳理了建筑表皮的发展历史,在总结建筑表皮职能的基础上提出复合表皮发展的必然性,并将复合表皮按照可变性分为静态表皮和动态表皮。在复合表皮节能的相关论述中,论文首先按照冷热负荷对办公建筑能耗进行了分析,随后提出办公建筑性能评价标准以及建筑表皮对建筑性能的影响方式,与建筑表皮结合可再生能源一起共同构成了复合表皮节能的理论依据;在此基础上,论文提出适用于夏热冬冷地区办公建筑复合表皮的设计策略,从复合形式、材料选择、几何系统设计、运动系统设计四个方面分析了复合表皮的设计方法。综合复合表皮节能理论与设计策略,论文以一拟建办公楼为案例进行了添加复合表皮前后的能耗模拟,按照从静态到动态的优化方式确定了复合表皮尺寸以及控制策略,结果表明,添加复合表皮可以使EUI降低19.8\%,对复合表皮进行动态控制设计可以使EUI降低22\%。},
  langid = {chinese},
  school = {郑州大学},
  keywords = {,Building energy efficiency,Composite skin design,Hot summer and cold winter area,Multi-objective optimization,Office building},
  annotation = {1 citations(CNKI)[3-20-2023]}
}

@article{carliIoTBasedArchitecture2020,

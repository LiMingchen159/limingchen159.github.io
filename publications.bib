@phdthesis{ZhangTianJinJingHaiQuNongCunMeiGaiDianGaiZaoXiangMuZongHeXiaoYiHouPingJieYanJiu2022,
  type = {{硕士}},
  title = {{天津静海区农村``煤改电''改造项目综合效益后评价研究}},
  author = {张, 颖},
  year = {2022},
  doi = {10.27431/d.cnki.gxnyu.2022.000089},
  abstract = {天津地区按``企业为主、政府推动、居民可承受''原则,加快推进``煤改电''项目建设。自2016年,天津市电力公司投资136.32亿元,大力开展``煤改电''改造项目,有效促进节能减排,特别是在淘汰污染大、效率低的产能方面有很大作用。众所周知``煤改电''工程的最终目的是将居民用户厨灶、取暖和工业锅炉中的使用煤炭改为应用电力,因此实施``煤改电''工程的主要对象为我国广大的农村地区,但是``煤改电''工程实施后,如此规模的建设任务到底为电网企业、为当地经济、为环境改善带来了什么样的影响?对项目开展后评价工作就显得尤为重要。天津静海区农村``煤改电''工程项目综合效益后评价的开展,首先通过文献阅读的方式,用更为科学、全面的模型,通过对比分析法对数据和指标进行对比分析,利用综合评价方法中的层次分析法构建指标体系递阶层级结构,确定本项目评级指标权重。从天津静海区农村``煤改电''工程整体的角度,分析评价工程目标的实现程度,可持续性和成功度。从电网性能、社会效益、环境效益以及企业经济等方面评价项目成功度、得出其对于这几个方面分别产生积极和消极方向的影响、主要经验和存在问题,特别是对社会效益和环境效益产生的影响,提出针对性的改进意见和建议,建议利用清洁能源改善环境,配套政策实施提高社会效益等。},
  langid = {chinese},
  school = {新疆农业大学},
  keywords = {,coal to electricity,economic benefits,environmental benefits,Post Evaluation,social benefits,system of evaluation}
}


@article{liOccupantCentricKeyPerformance2021,
  title = {Occupant-{{Centric}} Key Performance Indicators to Inform Building Design and Operations},
  author = {Li, Han and Wang, Zhe and Hong, Tianzhen},
  year = {2021},

@article{adanAutonomousRoboticPlatform2020,
  title = {An Autonomous Robotic Platform for Automatic Extraction of Detailed Semantic Models of Buildings},
  author = {Ad{\'a}n, A. and Quintana, B. and Prieto, S. A. and Bosch{\'e}, F.},
  year = {2020},

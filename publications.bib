  author = {Calleja Rodr{\'i}guez, Gloria and Carrillo Andr{\'e}s, Antonio and Dom{\'i}nguez Mu{\~n}oz, Fernando and Cejudo L{\'o}pez, Jos{\'e} Manuel and Zhang, Yi},

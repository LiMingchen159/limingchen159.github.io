@phdthesis{WangJiYuShenJingWangLuoDeWoGuoDianXingQiHouQuBanGongJianZhuXingNengDuoMuBiaoYouHuaYanJiu2021,
  type = {{硕士}},
  title = {{基于神经网络的我国典型气候区办公建筑性能多目标优化研究}},
  author = {王, 强},
  year = {2021},
  doi = {10.27157/d.cnki.ghzku.2021.006420},
  abstract = {为应对不断增长的建筑节能和环境品质改善需求,大力发展绿色建筑已成为社会共识。当前我国办公建筑存量大且新增量多,其能耗与环境品质性能的优化研究不容忽视。在建筑性能优化领域,性能优化已由单性能目标优化逐渐转为以多性能目标优化为主,而性能评估方法仍主要以模拟法居多。在多目标优化计算过程中,需要进行大量性能评估,采用模拟法导致该过程十分耗时,进一步导致了我国针对多气候区的建筑性能优化研究较少。为此,本文以神经网络方法替代传统的性能模拟评估过程,开展了针对我国五个典型气候区的办公建筑优化研究。多目标优化的性能目标为办公建筑的能耗和天然采光,基于对两个性能目标的分析,选择空间形态为优化的建筑对象。多目标优化流程主要包括三个步骤,首先通过抽样调查和文献调研两种方法建立典型办公建筑研究模型。其次,采用拉丁超立方采样和性能模拟获得``建筑形态--性能目标''数据集,并使用Matlab训练神经网络以建立形态与性能目标间的映射关系。最后,在Grasshopper平台耦合神经网络模型和多目标优化算法进行建筑形态的自动优化。研究在对多目标优化结果质量评价的基础上,采用Garson算法分析了形态参量的敏感性,并基于优化结果分析总结了优化设计策略。研究发现,对于我国五个典型气候区的办公建筑,各朝向窗高和南北向窗个数对于办公建筑能耗和采光性能普遍具有较高的敏感性。另外,能耗和采光性能导向的办公建筑形态参数设计,不同气候区存在不同的最佳取值区间。},
  langid = {chinese},
  school = {华中科技大学},
  keywords = {,building energy consumption,multi-objective optimization,Multi-storey office building,natural lighting,neural network}
}

@article{wangLearningOccupantsIndoor2020,
  title = {Learning Occupants' Indoor Comfort Temperature through a {{Bayesian}} Inference Approach for Office Buildings in {{United States}}},
  author = {Wang, Zhe and Hong, Tianzhen},
  year = {2020},
  month = mar,
  journal = {Renewable and Sustainable Energy Reviews},
  volume = {119},

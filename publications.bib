}

@phdthesis{RenJiYuJieNengDeHanDiBanGongJianZhuXingNengYouHuaMoXingGouJianYuYingYong2019,
  type = {{硕士}},
  title = {{基于节能的寒地办公建筑性能优化模型构建与应用}},
  author = {任, 惠},
  year = {2019},
  doi = {10.27061/d.cnki.ghgdu.2019.005866},
  abstract = {随着办公建筑面积占寒地城市建筑面积比重不断增加,办公建筑耗能所占比重不断增大。因此,寒地办公建筑节能设计已成为当下节能设计过程中亟待解决的问题。由于办公建筑设计方案遵循较固定的设计流程,建筑设计参量与建筑性能具有紧密联系,且建筑性能之间具有耦合作用(例如能耗与采光性能)。因此,本文构建的性能优化模型可通过构建集性能模拟、敏感性分析及多目标优化技术的整合平台,计算出合适的建筑设计参量组以较高效的进行性能优化。本文以优化建筑室内自然采光与能耗性能为目标,基于建筑性能模拟技术,建筑性能采样技术,设计参量敏感性分析技术及性能多目标优化技术,运用Python编程,将各类技术整合为以节能为主的寒地办公建筑性能优化模型。文中首先对建筑优化性能及相关性、设计参量与性能相关性、优化模型发展历史、基本理论与基本框架进行解析,为构建寒地办公建筑性能优化模型提供理论基础;其次通过文献综述对影响办公建筑采光与能耗性能的形态与非形态设计相关参量进行统计,并确定设计参量的组成类型与相应值域;再次,基于改进的办公建筑性能优化设计方法,制定建筑性能优化设计工作流,结合相应技术模块构建寒地办公建筑性能优化模型;最后结合项目实践探讨了寒地办公建筑性能优化模型的应用和验证。在论文中的实践案例中,通过设计基于单因素敏感分析法与基于性能优化模型的对比实验,可得出不同设计参量对建筑性能的敏感度及验证模型中敏感性分析模块的优越性,然后基于较优组分析所得的敏感性设计参量进行建筑性能多目标优化,并与场地既有建筑性能进行对比,结果显示基于建筑性能优化模型所得设计方案的室内采光与能耗性能更佳,较既有建筑有效天然采光照度百分比提升了2.48\%,年单位建筑面积耗能量节省了182.03k Wh/(m{\textasciitilde}2{$\cdot$}a)。最后,还对性能优化模型进行展望,希望能通过进一步的改进措施对该模型进行完善:如不同地区的气候设计参量有待进一步集成;模型还待进一步开发以实现对不同类型办公建筑的形态优化等。},
  langid = {chinese},
  school = {哈尔滨工业大学},
  keywords = {,building performance simulation,multi-objective optimization,multi-performance coupling relationship,performance optimization model,sensitivity analysis of design parameters},
  annotation = {1 citations(CNKI)[3-20-2023]}
}


  author = {Pilechiha, Peiman and Mahdavinejad, Mohammadjavad and Pour Rahimian, Farzad and Carnemolla, Phillippa and Seyedzadeh, Saleh},
  year = {2020},
  month = mar,
  journal = {Applied Energy},

@article{liuPredictedPercentageDissatisfied2020,
  title = {Predicted Percentage Dissatisfied with Vertical Temperature Gradient},
  author = {Liu, Shichao and Wang, Zhe and Schiavon, Stefano and He, Yingdong and Luo, Maohui and Zhang, Hui and Arens, Edward},
  year = {2020},
  month = aug,
  journal = {Energy and Buildings},
  volume = {220},
  pages = {110085},
  issn = {0378-7788},
  doi = {10.1016/j.enbuild.2020.110085},
  urldate = {2022-03-17},
  abstract = {A vertical thermally stratified environment provides opportunities for improved ventilation effectiveness and energy efficiency, but vertical temperature gradient can also cause local thermal discomfort. ASHRAE 55 and ISO 7730 prescribe a 3~{$^\circ$}C/m limit between head and feet for seated persons. However, an increasing amount of evidence suggests that this limit is too restrictive. To revisit how vertical temperature gradient affects local thermal comfort, we conducted laboratory tests with four nominal vertical temperature gradients (0.4, 2.9, 5.9, and 8.4~{$^\circ$}C/m). Ninety-eight seated college-age students participated in a blind within-subject experiment. Cold-feet discomfort is more frequently rated than warm-head discomfort with increasing temperature gradients. By using logistic regression modeling, we show that the whole-body dissatisfaction increases only slightly ({$<$}10\%) with vertical temperature gradient, even up to 8.4~{$^\circ$}C/m. Sex does not significantly affect the results except at 8.4~{$^\circ$}C/m. Acceptable vertical temperature gradient changes with thermal sensation votes. The results suggest that the vertical temperature gradient could be increased to 5~{$^\circ$}C/m between head and feet when the subject is thermally neutral.},
  langid = {english},
  keywords = {Displacement ventilation,Local thermal discomfort,Thermal comfort,Vertical temperature gradient,Whole body thermal sensation},
  annotation = {19 citations (Crossref/DOI) [2024-06-27]},
  file = {C\:\\Users\\liwei\\OneDrive\\Zotero\\王者论文\\2020\\Liu et al. - 2020 - Predicted percentage dissatisfied with vertical temperature gradient.pdf;C\:\\Users\\liwei\\Zotero\\storage\\MURTIXX4\\S0378778819332141.html}
}


  title = {Building {{HVAC}} Control with Reinforcement Learning for Reduction of Energy Cost and Demand Charge},
  author = {Jiang, Zhanhong and Risbeck, M. J. and Vish, Ramamurti and Murugesan, Sugumar and Jaume, Amores and Zhang, Chenlu and LeeYoung, M. and Drees, K. H.},
